Var:

    letra : carater

Algoritmo:

    selecionar letra:
        caso 'L': escrever(Ligar)
        caso 'D': escrever(Desligar)
        caso 'F': escrever(Furar)
        defeito: escrever(Erro)
    fimSelecionar

fimAlgoritmo