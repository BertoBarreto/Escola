/********************************\
**    Nome: Tipo de Triângulo   **
**    Autor: Roberto Barreto    **
**    Data: 01/10/2020          **
\********************************/

var:

    l1,l2,l3:real

Algoritmo:

    ler(l1,l2,l3)

    se l1<l2+l3 e l2<l1+l3 e l3<l1+l2 entao
        escrever(erro)
    senao se l1==l2==l3
        escrever(equilatero)
    senao se l1!=l2!=l3
        escrever(escaleno)
    senao
        escrever(isósceles)
    fimSe


fimAlgoritmo