var:

    mat,pt,ing,geo,media:reais

Algoritmo:

    ler(mat,pt,ing,geo)

    media<-(mat+pt+ing+geo)/4

    se media>=9.5 entao
        escrever(Aprovado)
    senao
        escrever(Reprovado)

fimAlgoritmo