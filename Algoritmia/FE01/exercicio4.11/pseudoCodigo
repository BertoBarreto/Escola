var:
    salarioB,salarioA,salarioF : real
algoritmo:
    ler(salarioB)
    salarioA<=salarioB+(0.05*salarioB)
    salarioF<=salarioA-(salario*0.07)

    escrever(salario,salarioA,salarioF)
    
fimAlgoritmo